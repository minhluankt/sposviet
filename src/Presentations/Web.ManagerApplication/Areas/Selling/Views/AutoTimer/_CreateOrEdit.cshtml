@using Domain.Entities
@model AutoSendTimer

<form id="create-form" method="post" asp-controller="AutoTimer" asp-action="OnPostCreateOrEdit" onsubmit="return jQueryModalPost(this);">
    @Html.HiddenFor(x=>x.Id)
    @Html.HiddenFor(x=>x.TypeSupplierEInvoice)
   <div class="row">
       <div class="col-md-8">
            <div class="formAddSupplierEInvoice">
                <div class="form-group">
                    <label for="exampleInputPassword1">Tên chương trình</label>
                    <input type="text" class="form-control" asp-for="Name" placeholder="Tên chương trình">
                </div>

                <div class="form-group">
                    <label for="exampleInputPassword1">Thời gian bắt đầu</label>
                    <input type="text" class="form-control" name="Time" placeholder="Thời gian bắt đầu">
                </div>

                <div class="form-group" style="display:flex;align-items:center">
                    <label class="m-0" for="exampleInputPassword1">Kích hoạt hệ thống</label>
                    <input class="form-control ml-3" asp-for="Active" style="width:20px;height:20px">
                </div>
            </div>
       </div>
       <div class="col-md-4">

       </div>
   </div>
</form>
