@using Domain.Entities
@model ManagerPatternEInvoice
@Html.HiddenFor(x=>x.Id)
@Html.HiddenFor(x=>x.IdSupplierEInvoice)
@Html.HiddenFor(x=>x.TypeSupplierEInvoice)
<div class="formAddSupplierEInvoice">
        <div class="form-group">
            <label for="exampleInputPassword1">Mẫu số hóa đơn</label>
            <input type="text" class="form-control" asp-for="Pattern" placeholder="Mẫu số hóa đơn, ví dụ: 1/001">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Ký hiệu hóa đơn</label>
            <input type="text" class="form-control" asp-for="Serial" placeholder="Ký hiệu hóa đơn ví dụ: C23TAB">
        </div>
        <div class="form-group" style="display:flex;align-items:center">
            <label class="m-0" for="exampleInputPassword1">Hiển thị mặc định</label>
            <input class="form-control ml-3" asp-for="Selected" style="width:20px;height:20px">
        </div>
        <div class="form-group" style="display:flex;align-items:center">
        <label class="m-0" for="exampleInputPassword1">Kích hoạt hiển thị</label>
        <input class="form-control ml-3" asp-for="Active" style="width:20px;height:20px">
        </div>
</div>